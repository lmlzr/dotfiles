{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "file",
    "source": "~/.config/fastfetch/logo.txt",
    "color": {
        "1": "#bb0b41",
      //  "2": "#ec3e5f4",
      //  "3": "#ec3e5f4",
      //  "4": "#ec3e5f4",
      //  "5": "#ec3e5f4",
      //  "6": "#ec3e5f4",
      //  "7": "#ec3e5f4",
      // "8": "#ec3e5f4",
      // "9": "#ec3e5f4",
    },
    "padding": {
      "top": 1,
      "right": 3
    }
  },
  "display": {
    "separator": " "
  },
  "modules": [
    "break",
    {
      "type": "title",
      "key": " ",
      "keyIcon": "",
      "fqdn": false,
      "color": {
        "user": "#660B2A",
        "at": "",
        "host": "#bb0b41"
      }
    },
    "break",
    {
      "type": "custom",
      "format": "{##bb0b41}Software Information"

    },
    {
      "type": "os",
      "key": "",
      "keyColor": "#660B2A"
    },
    {
      "type": "kernel",
      "key": "",
      "keyColor": "#660B2A"
    },
    {
      "type": "packages",
      "key": "󰏖",
      "keyColor": "#660B2A",
      "disabled": [
        "winget"
      ],
      "combined": false
    },
        {
      "type": "shell",
      "key": "",
      "keyColor": "#660B2A"
    },
            {
      "type": "terminal",
      "key": "󰄛",
      "keyColor": "#660B2A"
    },
    "break",
    {
      "type": "custom",
      "format": "{##bb0b41}Hardware Information"
    },
    {
      "type": "cpu",
      "key": "",
      "keyColor": "#660B2A"
    },
    {
      "type": "gpu",
      "key": "󰨇",
      "driverSpecific": false,
      "detectionMethod": "pci",
      "temp": false,
      "hideType": "none",
      "keyColor": "#660B2A"
    },
    {
      "type": "board",
      "key": "󰚗",
      "keyColor": "#660B2A"
    },
    {
      "type": "memory",
      "key": "",
      "keyColor": "#660B2A",
      "format": "{used} / {total} ({percentage})"
    },
    {
      "type": "processes",
      "key": " ",
      "keyIcon": "",
      "keyColor": "#660B2A"
    },
    "break",
    {
      "type": "custom",
      "format": "{##bb0b41}Time Information"
    },    
    {
      "type": "uptime",
      "key": "",
      "keyColor": "#660B2A"
    },
    {
      "type": "datetime",
      "key": "",
      "keyColor": "#660B2A"
    },    
    // TODO system age based on https://www.reddit.com/r/fastfetch/comments/1loyaks/first_ricing/?utm_source=share&utm_medium=web3x&utm_name=web3xcss&utm_term=1&utm_content=share_button
    "break",
    {
      "type": "colors",
      "symbol": "circle"
    }
  ]
}